{
  "name": "rr-shadcn-theme",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.8.1",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm typegen && NODE_ENV=production vite build",
    "preview": "vite preview",
    "node:build": "NODE_ENV=production react-router build",
    "node:dev": "react-router dev",
    "node:start": "react-router-serve ./build/server/index.js",
    "deploy:cloudflare": "wrangler deploy",
    "typegen": "wrangler types && react-router typegen",
    "typecheck": "wrangler types && react-router typegen && pnpm tsc --project tsconfig.json --noEmit",
    "shadcn": "pnpm dlx shadcn@latest",
    "postshadcn": "pnpm lint:fix",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "reset": "pnpm dlx rimraf --glob ./**/node_modules && pnpm dlx rimraf --glob ./**/{dist,build}"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-slot": "^1.2.0",
    "@react-router/fs-routes": "^7.5.2",
    "@react-router/node": "^7.5.2",
    "@react-router/serve": "^7.5.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "isbot": "^5.1.17",
    "lucide-react": "^0.503.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.5.2",
    "remix-utils": "^8.5.0",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.8",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/vite-plugin": "^1.0.12",
    "@react-router/dev": "^7.5.2",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.15.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.13.2"
  }
}
